wmsuatrestart() {

  SAVEIFS=$IFS
  IFS=$'\n'

  services=('wms-service' 'dashboard' 's3f-wms-service', 'submit-po-to-adaptris', 'adaptris-service')

  for i in $services; do
    wms=$(kubectl --context kubernetes-admin@kubernetes --namespace wmsuat get pods -o name | grep -v s3f | grep $i | sed 's/pods\///')
    echo $wms | xargs -I % sh -c '$UAT delete pod % '
  done

  IFS=$SAVEIFS

}
